{
  "name": "take-a-break-from-coding",
  "publisher": "Amjad",
  "displayName": "Take a break from coding",
  "description": "A reminder to take a break",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "icon": "icon.png",
  "main": "./extension.js",
  "contributes": {
    "configuration":[
      {
        "title": "Time of the break",
        "properties": {
          "timeOf.coding": {
            "type": "number",
            "default": 30,
            "description": "Time in minutes"
          },
          "timeOf.break": {
            "type": "number",
            "default": 10,
            "description": "Time in minutes"
          },
          "timeOf.snooze": {
            "type": "number",
            "default": 5,
            "description": "Time in minutes"
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
